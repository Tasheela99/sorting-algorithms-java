<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox spacing="20" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.assignment.programming3assignment.controllers.AnalysisScreenController">

    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <TableView fx:id="columnTableView" maxWidth="500.0" minWidth="300.0" prefWidth="350.0" style="-fx-font-family: 'Comfortaa SemiBold';" HBox.hgrow="SOMETIMES" />

    <VBox spacing="15" HBox.hgrow="ALWAYS">
        <VBox spacing="10">
            <HBox alignment="CENTER_LEFT" spacing="10">
                <Button fx:id="mergeSortButton" minHeight="50.0" minWidth="120.0" prefHeight="50.0" prefWidth="292.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #a4b0be; -fx-text-fill: #2f3542; -fx-border-radius: 10;" text="Merge Sort">
                    <font>
                        <Font size="15.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                </Button>
                <Label fx:id="mergeSortTimeLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="150.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-border-color: #a4b0be; -fx-border-width: 1; -fx-padding: 5; -fx-border-radius: 10;" textAlignment="CENTER">
                    <font>
                        <Font size="20.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="DISAPPEAR" />
               </cursor>
                </Label>
            </HBox>

            <HBox alignment="CENTER_LEFT" spacing="10">
                <Button fx:id="quickSortButton" minHeight="50.0" minWidth="120.0" prefHeight="50.0" prefWidth="292.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #a4b0be; -fx-text-fill: #2f3542; -fx-border-radius: 10;" text="Quick Sort">
               <font>
                  <Font size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
                <Label fx:id="quickSortTimeLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="150.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-border-color: #a4b0be; -fx-border-width: 1; -fx-padding: 5; -fx-border-radius: 10;" textAlignment="CENTER">
                    <font>
                        <Font size="20.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="DISAPPEAR" />
               </cursor>
                </Label>
            </HBox>

            <HBox alignment="CENTER_LEFT" spacing="10">
                <Button fx:id="insertionSortButton" minHeight="50.0" minWidth="120.0" prefHeight="50.0" prefWidth="291.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #a4b0be; -fx-text-fill: #2f3542; -fx-border-radius: 10;" text="Insertion Sort">
               <font>
                  <Font size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
                <Label fx:id="insertionSortTimeLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="150.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-border-color: #a4b0be; -fx-border-width: 1; -fx-padding: 5; -fx-border-radius: 10;" textAlignment="CENTER">
                    <font>
                        <Font size="20.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="DISAPPEAR" />
               </cursor>
                </Label>
            </HBox>

            <HBox alignment="CENTER_LEFT" spacing="10">
                <Button fx:id="shellSortButton" minHeight="50.0" minWidth="120.0" prefHeight="50.0" prefWidth="291.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #a4b0be; -fx-text-fill: #2f3542; -fx-border-radius: 10;" text="Shell Sort">
               <font>
                  <Font size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
                <Label fx:id="shellSortTimeLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="150.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-border-color: #a4b0be; -fx-border-width: 1; -fx-padding: 5; -fx-border-radius: 10;" textAlignment="CENTER">
                    <font>
                        <Font size="20.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="DISAPPEAR" />
               </cursor>
                </Label>
            </HBox>

            <HBox alignment="CENTER_LEFT" spacing="10">
                <Button fx:id="heapSortButton" minHeight="50.0" minWidth="120.0" prefHeight="50.0" prefWidth="291.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #a4b0be; -fx-text-fill: #2f3542; -fx-border-radius: 10;" text="Heap Sort">
               <font>
                  <Font size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
                <Label fx:id="heapSortTimeLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="150.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-border-color: #a4b0be; -fx-border-width: 1; -fx-padding: 5; -fx-border-radius: 10;" textAlignment="CENTER">
                    <font>
                        <Font size="20.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="DISAPPEAR" />
               </cursor>
                </Label>
            </HBox>

            <Button fx:id="showBestButton" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="500.0" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #a4b0be; -fx-text-fill: #2f3542; -fx-border-radius: 10;" text="Show Best Algorithm" textFill="WHITE">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
        </VBox>

        <BarChart fx:id="sortingTimeBarChart" alternativeColumnFillVisible="true" animated="false" focusTraversable="true" style="-fx-font-family: 'Comfortaa SemiBold'; -fx-background-color: #ffffff;" VBox.vgrow="ALWAYS">
            <xAxis>
                <CategoryAxis animated="false" label="Sorting Algorithm" style="-fx-font-family: 'Comfortaa SemiBold';" />
            </xAxis>
            <yAxis>
                <NumberAxis animated="false" autoRanging="true" label="Time (ms)" style="-fx-font-family: 'Comfortaa SemiBold';" />
            </yAxis>
         <effect>
            <Glow />
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
        </BarChart>
      <cursor>
         <Cursor fx:constant="OPEN_HAND" />
      </cursor>
    </VBox>
</HBox>
